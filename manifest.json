{
  "name": "Clockstopper",
  "version": "1.2",
  "description": "F5 or ALT + D for debugger",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["script.js"],
      "run_at": "document_start"
    }
  ],
  "browser_action": {
    "default_icon": "stopwatch.png"
  },
  "manifest_version": 2
}
